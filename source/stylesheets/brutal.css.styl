@charset "utf-8";
html
  height 100%

body
  font-family Hapna
  font-weight normal
  font-style normal
  position relative
  background #fffcde
  &.about
    #logo
      background transparent url(../images/logo.png)
  a
    &.lang_icon
      width 12px
      height 12px
      position absolute
      right 15px
      top 15px
      display block
      padding 10px 0px
      background-image url("/images/lang_icon.png")
      background-repeat no-repeat
      background-position -12px 0px
      &.ru
        background-position 0px 0px
        display block
  &.ru
    .ru
      display block
    .eng
      display none
  &.eng
    .ru
      display none
  .ru
    display none

*
  box-sizing border-box

#page_switch
  float right
  margin -2px 50px 0px 0px

#top_nav
  background #1a1a1a
  padding 10px 0px
  position fixed
  left 0
  right 0
  z-index 2
  a
    color #fffcde
    text-decoration none
    font-size 18px
    font-family Hapna
    font-weight bold 
    font-style normal
#wall
  ._tile
    font-size 36px
    font-weight bold
    color #211b1f

#container
  width 100%
  margin 0px auto
  overflow hidden

#logo
  display block
  margin 40px auto 20px
  width 135px
  height 92px
  background transparent url(../images/logo3.png)
  position relative

nav[role="navigation"]
  ul
    li
      float left
      width 50%
      margin 14px 0 0 0
      &.last
        margin-right 0
      a
        display block
        float left
        width 100%
        font-size 1.5em
        line-height 1.3em
        text-decoration none
        text-transform uppercase
        letter-spacing 3px
        strong
          display block
          padding 0
          font-weight bold
          color rgba(22, 22, 22, 1)
          transition color 0.2s ease-in-out
        em
          display block
          font-size .95em
          font-weight normal
          font-style normal
          text-transform none
          letter-spacing 0
          color rgba(22, 22, 22, 1)
          transition color 0.2s ease-in-out

body.about  nav[role="navigation"] ul li a strong,
body.ru_about  nav[role="navigation"] ul li a strong
  color rgba(21, 214, 111, 0.6)

body.about  nav[role="navigation"] ul li a em,
body.ru_about  nav[role="navigation"] ul li a em
  color rgba(21, 214, 111, 0.46)

nav[role="navigation"] ul li a:hover strong,
nav[role="navigation"] ul a.active strong
  color rgba(22, 22, 22, 1)

nav[role="navigation"] ul li a:hover em,
nav[role="navigation"] ul a.active em
  color rgba(22, 22, 22, 1)

body.about nav[role="navigation"] ul li a:hover strong,
body.about nav[role="navigation"] ul a.active strong,
body.ru_about nav[role="navigation"] ul li a:hover strong,
body.ru_about nav[role="navigation"] ul a.active strong
  color rgba(21, 214, 111, 1)

body.about  nav[role="navigation"] ul li a:hover em,
 body.about nav[role="navigation"] ul a.active em,
 body.ru_about  nav[role="navigation"] ul li a:hover strong,
 body.ru_about nav[role="navigation"] ul a.active strong
  color rgba(21, 214, 111, 0.7)

#start
  border-radius 18px
  background-clip padding-box
  background-color #4c307b
  box-shadow 0 2px 5px rgba(0,0,0,.11)
  background-image linear-gradient(90deg, rgba(0,0,0,.3) 0%, rgba(0,0,0,0) 100%)
  color #e5e5e5
  text-shadow 0 1px 0 rgba(24,10,48,.75)
  font-size 31px
  text-transform uppercase
  height 50px
  width 200px
  line-height 50px
  display block
  font-weight 700
  margin 50px auto
  cursor pointer

nav
  overflow hidden
  width 600px
  margin 20px auto
  ul
    padding 0px

#promo
  width 100%
  height 200px
  background-color #221d1d
  margin-top 60px
  padding 60px 0px 110px
  .text
    font-size 40px
    text-transform uppercase
    color #e5e5e5
    font-weight 600
    letter-spacing 2px

#content
  padding 0px 8%

.inner_content
  width 800px
  margin 250px auto 0px
  padding-bottom 50px


a
  &.gallery_img
    font-weight bold
    color #777
    text-decoration none
    text-align right
    margin 8px 8px 16px 8px
    text-transform uppercase
    font-size 11px
    width 784px
    display block
    margin 0px auto
    box-shadow 0px 0px 2px rgba(50, 50,50, 0.6), inset 0px 0px 8px rgba(50, 50, 50, 0.1)
    padding 20px 20px 10px
  &.gallery_img_next
    position relative
    overflow hidden
    border-top-left-radius 3px
    border-top-right-radius 3px
    display block
    text-decoration none

.gal_img
  display block

.header
  overflow hidden
  padding-bottom 100px
  background unquote("url(data:image/svg+xml;base64,)")
  background -moz-linear-gradient(top, rgba(255,252,222,1) 62%, rgba(255,252,222,0.79) 70%, rgba(255,255,255,0) 100%)
  background -webkit-gradient(linear, left top, left bottom, color-stop(62%,rgba(255,252,222,1)), color-stop(70%,rgba(255,252,222,0.79)), color-stop(100%,rgba(255,255,255,0)))
  background -webkit-linear-gradient(top, rgba(255,252,222,1) 62%,rgba(255,252,222,0.79) 70%,rgba(255,255,255,0) 100%)
  background -o-linear-gradient(top, rgba(255,252,222,1) 62%,rgba(255,252,222,0.79) 70%,rgba(255,255,255,0) 100%)
  background -ms-linear-gradient(top, rgba(255,252,222,1) 62%,rgba(255,252,222,0.79) 70%,rgba(255,255,255,0) 100%)
  background linear-gradient(to bottom, rgba(255,252,222,1) 62%,rgba(255,252,222,0.79) 70%,rgba(255,255,255,0) 100%)
  filter unquote("progid:DXImageTransform.Microsoft.gradient( startColorstr='#fffcde', endColorstr='#00ffffff',GradientType=0 )")

  position fixed
  left 0
  right 0
  z-index 2
  top 43px
  padding-top 17px

#about_us
  display none
  opacity 0
  .header
    background unquote("url(data:image/svg+xml;base64,)")
    background -moz-linear-gradient(top,  rgba(25,25,25,1) 0%, rgba(33,33,33,1) 64%, rgba(34,34,34,0.77) 76%, rgba(35,35,35,0.29) 86%, rgba(37,37,37,0) 100%)
    background -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(25,25,25,1)), color-stop(64%,rgba(33,33,33,1)), color-stop(76%,rgba(34,34,34,0.77)), color-stop(86%,rgba(35,35,35,0.29)), color-stop(100%,rgba(37,37,37,0)))
    background -webkit-linear-gradient(top,  rgba(25,25,25,1) 0%,rgba(33,33,33,1) 64%,rgba(34,34,34,0.77) 76%,rgba(35,35,35,0.29) 86%,rgba(37,37,37,0) 100%)
    background -o-linear-gradient(top,  rgba(25,25,25,1) 0%,rgba(33,33,33,1) 64%,rgba(34,34,34,0.77) 76%,rgba(35,35,35,0.29) 86%,rgba(37,37,37,0) 100%)
    background -ms-linear-gradient(top,  rgba(25,25,25,1) 0%,rgba(33,33,33,1) 64%,rgba(34,34,34,0.77) 76%,rgba(35,35,35,0.29) 86%,rgba(37,37,37,0) 100%)
    background linear-gradient(to bottom,  rgba(25,25,25,1) 0%,rgba(33,33,33,1) 64%,rgba(34,34,34,0.77) 76%,rgba(35,35,35,0.29) 86%,rgba(37,37,37,0) 100%)
    filter unquote("progid:DXImageTransform.Microsoft.gradient( startColorstr='#191919', endColorstr='#00252525',GradientType=0 )")


#bgg
  background #fffcde
  position absolute
  left 0px
  right 0px
  height 134px

.cari
  box-shadow 8px 10px 0px rgba(3,3,3,0.3)
  text-align center
  margin 0px auto 130px
  position relative
  border-radius 3px
  border 0px solid #fff
  .bottomt
    background #111
    height 51px
    border-bottom-left-radius 3px
    border-bottom-right-radius 3px
    box-shadow 0px -1px 6px rgba(1,1,1,0.2) inset
  span
    &.title
      position absolute
      right 5px
      bottom 44px
      display none
    &.view_icon
      position absolute
      left 5px
      bottom 44px
      display none
  a
    span
      bottom 5px
      right 5px
      color #fff

span
  &.number
    font-family  Hapna
    text-transform uppercase
    color #515151
    font-size 22px
    font-weight normal
    letter-spacing 4px
    line-height 50px

h2
  font-size 38px
  margin-bottom 30px
  font-family HapnaSlabSerif-ExtraBold
body.about h2,
body.ru_about h2
  color #e5e5e5

body.about h5,
body.ru_about h5
  font-size 24px

.post
  padding 20px 40px
  margin 0 auto
  width 100%
  display block
  height 100%
  opacity 1
  position absolute
  top 43px
  left 0px
  text-align left
  font-size 20px
  line-height 30px
  font-size 16px
  padding 20px 10px
  width 100%

body.about .post,
body.ru_about .post
  background #252525
  color #e5e5e5

body.about,
body.ru_about
  background #252525

body.about .post a,
body.ru_about .post a
  color rgba(21, 214, 111, 1)
  text-decoration underline

@media only screen and (min-device-width : 320px) and (max-device-width : 480px)
  #bottomsection .icon-move-horizontal
    padding 0 15px
    //(max-width : 720px)
  nav
    width 100%
  @font-face
    font-family 'mp'
    font-weight normal
    font-style normal

.letterbox
  padding 0px 0px 0px 0px
  background #211b1f
  border-radius 2px
  overflow hidden
  width 540px
  height 320px
  &.single
    width 500px
    height 300px
    margin 0 auto
  &.full
    width 100%
    position absolute
    left 0px
    right 0px
    margin 0px
  img
    margin 0 auto
    padding 0px
    display block
    &.fullimage
      display none

#footer
  padding 50px 0px
  font-size 15px
  color #fff
  background rgba(231,32,32, 1)
  .mail
    font-size 48px
    font-weight bold
    color #fff
    text-decoration none
    text-shadow 0 2px 3px rgba(0,0,0,.12)
  .icon
    width 48px
    height 48px
    text-decoration none
    color #fff
    text-shadow 0 2px 3px rgba(0,0,0,.12)
    font-size 37px
    padding 0px 5px
  span
    display block

body.about #footer,
body.ru_about #footer
  color #e5e5e5

#loadmore
  text-align center
  text-transform uppercase
  opacity 0.9
  font-size 30px
  letter-spacing 4px
  color #fff
  background #111
  padding 10px 0px
  margin 0 -10%

.test
  #overlay
    background rgba(21, 214, 111, 0.2) 0 0 repeat

html
  body
    &.test
      & > .wrapper
        filter blur(8px)

.close
  position absolute
  top 0px
  right 0px
  opacity 1
  width 50px
  height 50px
  filter unquote('alpha(opacity=100)')
  z-index 2
  & > span
    position absolute
    right 20px
    top 20px
    font-size 60px
    font-weight 400
    color #fff
    text-shadow none
    float right
    opacity .3
    filter unquote('alpha(opacity=30)')
    cursor pointer
    line-height 20px

#overlay
  position fixed
  left 0
  right 0
  bottom 0
  display block
  top 0
  z-index 9999
  height 100%
  width 100%
  overflow hidden
  z-index 1000
  text-align center
  white-space nowrap
  background #211b1f 0 0 repeat
  transition opacity .2s ease-in-out
  perspective 200
  transform-style preserve-3d
  display none
  &:before
    content ''
    display inline-block
    height 100%
    vertical-align middle
    margin-right -0.25em
    font-size 0
  & > section
    display inline-block
    vertical-align middle
    white-space normal
    opacity 1
    filter unquote('alpha(opacity=100)') 
    &.module-piece-detail
      .media
        max-width 980px
        max-height 100%
        text-align right
        position relative
  &.loaded
    & > .loading
      & > section
        overflow hidden
        background-color #211b1f
        position absolute
        position a
        left 0
        right 0
        top 0
        bottom 70px
        .media
          height 100%
          overflow hidden
          display block
  .loading
    #small_logo
      width 80px
      height 48px
      display block
      text-decoration none
      background transparent url('/images/micro-logo2.png') no-repeat
      position absolute
      z-index 3
      top 21px
      left 21px
    #bottomsection
      background #fff
      height 70px
      position absolute
      bottom -70px
      left 0
      right 0
  *
    user-select none
  img
    pointer-events none
  #bottomsection
    height 50px

#overlay > .loading > section.module-piece-detail .media, #overlay  > .loading > section.module-piece-detail .media + aside
  display inline-block
  vertical-align middle
  white-space normal

.media
  img
    border-radius 7px

#bottomsection
  .icon
    color #000
    text-decoration none
    font-size 70px
    line-height 66px
  #prev-image
    float right
    padding 0px 30px
  #next-image
    float right
    padding 0px 30px
  span.number
    line-height 73px
    float left
    padding-left 30px
#bottomsection .icon.disabled
  cursor not-allowed
  color #777

#prev-image
  span
    border-right 20px solid #000

#next-image
  span
    border-left 20px solid #000

.wrapper
  min-height 100%
  height auto!important
  height 100%
  margin-bottom -50px

@media only screen and (-webkit-min-device-pixel-ratio : 1.5), only screen and (min-device-pixel-ratio : 1.5)
  .post
    padding 20px 10px

#aaa
  display none

.lazy
  display none
  vertical-align top

.loading
  width 100%
  height 100%

@media only screen and (min-device-width : 768px) and (max-device-width : 1024px)
  .post
    padding 20px 80px
  .inner_content
    width auto
    .cari a
      height 240px
      img
        margin-top -210px

